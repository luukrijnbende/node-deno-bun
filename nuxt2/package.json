  "author": "Luuk Rijnbende",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "node node_modules/nuxt/bin/nuxt.js build",
    "start:node": "node node_modules/nuxt/bin/nuxt.js start",
    "start:deno": "deno run --unstable --compat --allow-all node_modules/nuxt/bin/nuxt.js start",
    "start:bun": "bun run node_modules/nuxt/bin/nuxt.js start"
  },
  "dependencies": {
    "core-js": "^3.19.3",
